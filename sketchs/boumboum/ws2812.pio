.program ws2812
.side_set 1 opt

; WS2812 DEBUG - compatible RP2350
; Chaque bit est envoyé via out pins,1
; Le timing encode 0 ou 1

.wrap_target
pull block
out x, 24        ; 24 bits dans X

bitloop:
    out pins, 1  side 1   ; sort le bit MSB → HIGH
    nop         side 1    ; T1H ou T0H (bit=1 → HIGH plus long)
    nop         side 0    ; T1L ou T0L (bit=0 → LOW plus long)
    jmp x--, bitloop
.wrap


















