.program ws2812
.side_set 1 opt

.wrap_target
bitloop:
    out x, 1        side 0 [0]
    jmp !x do_zero  side 1 [1]
    jmp    do_one   side 1 [1]

do_zero:    nop side 1 [2]   ; haut 4 cycles ≈ 0,416 µs
            nop side 0 [5]   ; bas 8 cycles ≈ 0,832 µs
            jmp bitloop side 0 [1]

do_one:     nop side 1 [4]   ; haut 8 cycles ≈ 0,832 µs
            nop side 0 [1]  
            jmp bitloop side 0 [1] 
.wrap


